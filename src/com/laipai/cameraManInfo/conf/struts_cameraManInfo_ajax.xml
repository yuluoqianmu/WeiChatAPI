<struts>
	<package name="cameraManAjax" extends="json-default">
    <action name="editCameraman" class="cameraManAction" method="editCameraman">
          <result type="json">
          <param name="root">jsonObject</param>
          </result>
    </action> 
    <action name="disableCamera" class="cameraManAction" method="disableCamera">
          <result type="json">
          <param name="root">jsonObject</param>
          </result>
    </action>
    <action name="loadMoreGalary" class="cameraManAction" method="loadMore">
    <result type="json" name="success">
          <param name="root">jsonObject</param>
          </result>
    </action>
   <action name="loadMorePictures" class="cameraManAction" method="loadMorePictures">
    <result type="json" name="success">
          <param name="root">jsonObject</param>
          </result>
    </action>
      <action name="enableCamera" class="cameraManAction" method="enableCamera">
          <result type="json">
          <param name="root">jsonObject</param>
          </result>
    </action>
    
    <action name="changePSW" class="cameraManAction" method="changePSW">
          <result type="json">
          <param name="root">jsonObject</param>
          </result>
    </action>
       
	</package>
</struts>